    "knex": "^0.21.12",
    "morgan": "^1.10.0",
    "nanoid": "^3.1.16",
    "pg": "^8.0.3",
    "postgrator-cli": "^3.3.0",
    "socket.io": "^2.3.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.10",
